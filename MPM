<script type='text/javascript'>
//<![CDATA[
$(window).scroll(function() {
    if($(this).scrollTop() > 200) {
        $('#top-nav').show();
    } else {
        $('#top-nav').hide();
    }
});
$('#top-nav').hide().click(function() {
    $('html, body').animate({scrollTop:0}, 1000);
    return false;
});
//]]>
</script>

